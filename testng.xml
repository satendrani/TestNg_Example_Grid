<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="methods" thread-count="5">

    <listeners>
        <listener class-name="com.listeners.TestJiraListener"></listener>
    </listeners>

    <test thread-count="5" name="Selenium Grid Test - Parallel Execution">
        <groups>
            <run>
                <include name="sanity"/>
            </run>
        </groups>
        <classes>
            <class name="GridDemo_GoogleHomePageTest"/>
            <class name="SauceLabsConnection"/>
            <class name="ReverseStringTest"/>
        </classes>
    </test> <!-- Test -->
</suite> <!-- Suite -->