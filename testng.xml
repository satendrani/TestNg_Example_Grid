<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="methods" thread-count="5">
    <listeners>
                <listener class-name="com.listeners.ReporterListener"></listener>
    </listeners>

    <test thread-count="5" name="Selenium Grid Test - Parallel Execution">
        <groups>
            <run>
                <include name="sanity"/>
            </run>
        </groups>
        <classes>
            <class name="tests.GridDemo_GoogleHomePageTest"/>
            <class name="tests.SauceLabsConnection"/>
            <class name="tests.ReverseStringTest"/>
            <class name="com.mypages.ExtentReportDemo1"/>
            <class name="com.mypages.ExtentReportDemo2"/>
            <class name="com.mypages.ExtentReportDemo3"/>
        </classes>
    </test> <!-- Test -->
</suite> <!-- Suite -->